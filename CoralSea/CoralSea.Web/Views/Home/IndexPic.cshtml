
@{
    ViewBag.Title = "IndexPic";
}
@Scripts.Render("~/Scripts/fullpage.js/js/jquery.fullPage.min.js")
@Styles.Render("~/Scripts/fullpage.js/css/jquery.fullPage.css")
<div id="dowebok">
    <div class="section">
        <div class="slide" style="text-align: center;background-color: #4BBFC3"></div>
        <div class="slide" style="text-align: center;background-color: #7BAABE"></div>
        <div class="slide" style="text-align: center;background-color: #f90"></div>
        <div class="slide" style="text-align: center;background-color: #1bbc9b"><a href="@Url.Content("~/Home/NewBaseInfo")">点击图片进入维护基本信息...</a></div>
    </div>
</div>
<script type="text/javascript">
    $(function () {
        $('#dowebok').fullpage({
            slidesColor: ['#1bbc9b', '#4BBFC3', '#7BAABE', '#f90']
        });
    });
</script>

